{
  "name": "babel-plugin-pretty-log",
  "version": "1.0.1",
  "description": "A pretty log for console.log in browser and nodejs",
  "author": "xxxxxMiss",
  "license": "MIT",
  "repository": "xxxxxMiss/babel-plugin-pretty-log",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "commit": "git-cz",
    "clean": "rimraf dist",
    "release": "yarn clean && yarn build && standard-version && npm publish",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-tester": "^9.2.0",
    "codecov": "^3.7.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^24.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-url": "^3.0.1",
    "standard-version": "^8.0.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/*.spec.{js,jsx}"
    ],
    "coverageDirectory": "./coverage/",
    "errorOnDeprecated": true,
    "globals": {},
    "verbose": true
  }
}
